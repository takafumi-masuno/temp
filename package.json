    "prepare": "husky install",
    "build": "nx run at-conpane:build",
    "start": "nx serve at-conpane",
    "start:port": "nx serve at-conpane --port",
    "build:bff": "nx build at-bff --skip-nx-cache",
    "start:bff": "nx serve at-bff",
    "prettier": "prettier . --write",
    "eslint": "npm run eslint:apps && npm run eslint:libs",
    "eslint:apps": "npm run eslint:bff && npm run eslint:customhouse:conpane",
    "eslint:bff": "eslint ./apps/at-bff --fix",
    "eslint:customhouse:conpane": "eslint ./apps/at-conpane --fix",
    "eslint:libs": "npm run eslint:libs:bff && npm run eslint:libs:front && npm run eslint:libs:shared",
    "eslint:libs:bff": "eslint ./libs/bff --fix",
    "eslint:libs:front": "eslint ./libs/front --fix",
    "eslint:libs:shared": "eslint ./libs/shared --fix",
    "generate:bff": "npx nest g s %npm_config_name% --project=athome-customhouse-conpane-bff --no-spec && npx nest g mo %npm_config_name% --project=athome-customhouse-conpane-bff && npx nest g co %npm_config_name% --project=athome-customhouse-conpane-bff --no-spec"
